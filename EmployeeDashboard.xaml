<Window x:Class="HR_Management_System.EmployeeDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HR_Management_System"
        mc:Ignorable="d"
        Title="EmployeeDashboard" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="101,174,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Name="txtaddress"/>
        <TextBox HorizontalAlignment="Left" Margin="236,277,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Margin="261,151,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Name="txtcontactinfodash"/>
        <TextBox HorizontalAlignment="Left" Margin="101,277,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Margin="236,254,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Margin="101,128,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Name="txtfirstnamedash"/>
        <TextBox HorizontalAlignment="Left" Margin="101,151,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Name="txtDOBdash"/>
        <TextBox HorizontalAlignment="Left" Margin="101,104,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="280" Name="txtIDdash"/>
        <TextBox HorizontalAlignment="Left" Margin="261,128,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Name="txtlastnamedash"/>
        <TextBox HorizontalAlignment="Left" Margin="261,174,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Name="txtcitydash"/>
        <TextBox HorizontalAlignment="Left" Margin="101,254,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblempdashboard" Content="Employee Dashboard" HorizontalAlignment="Left" VerticalAlignment="Top" Width="267" FontFamily="Arial Black" FontSize="22"/>
        <Button x:Name="btnprofilepic" Content="" HorizontalAlignment="Left" Margin="673,18,0,0" VerticalAlignment="Top" Height="69" Width="91">
            <Button.Background>
                <ImageBrush ImageSource="/images.jpg"/>
            </Button.Background>
        </Button>
        <Border BorderBrush="Black" BorderThickness="1" Margin="652,-10,0,0">
            <Label Content="Reports To" Margin="9,122,35,291" FontWeight="Bold"/>
        </Border>
        <Label Content="Upcoming Leave" HorizontalAlignment="Left" Margin="101,337,0,0" VerticalAlignment="Top" Width="270" FontWeight="Bold"/>
        <Label Content="Personal Details" HorizontalAlignment="Left" Margin="101,61,0,0" VerticalAlignment="Top" Width="270" FontWeight="Bold"/>
        <Label Content="Company Details" HorizontalAlignment="Left" Margin="101,217,0,0" VerticalAlignment="Top" Width="270" FontWeight="Bold"/>
        <TextBox HorizontalAlignment="Left" Margin="338,35,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="243" Height="39" Name="txtgodash" TextChanged="txtgodash_TextChanged#"/>

    </Grid>
</Window>
